<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import jfxtras.scene.control.agenda.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ScheduleController">
   <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="20.0" prefHeight="20.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="20.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Agenda fx:id="agenda" GridPane.halignment="CENTER" GridPane.valignment="CENTER" GridPane.columnSpan="2"/>
      <Button mnemonicParsing="false" onAction="#moveToNextWeek" text="Next Week" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
      <Button mnemonicParsing="false" onAction="#moveToPrevWeek" text="Previous Week" GridPane.columnIndex="0" GridPane.halignment="LEFT" GridPane.rowIndex="2" />
   </children>
   <padding>
      <Insets bottom="9.0" left="9.0" right="9.0" top="9.0" />
   </padding>
</GridPane>
