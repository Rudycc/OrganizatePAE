<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="classGridPane" hgap="15.0" vgap="20.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="45.0" minWidth="45.0" prefWidth="45.0" />
    <ColumnConstraints hgrow="SOMETIMES" />
    <ColumnConstraints hgrow="SOMETIMES" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="6.0" prefHeight="6.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="6.0" prefHeight="6.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="6.0" prefHeight="6.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <padding>
      <Insets bottom="9.0" right="0.0" top="9.0" />
   </padding>
   <children>
      <Label styleClass="upper-label,text-bold" fx:id="lblClass" text="Label" GridPane.columnIndex="1" />
      <Label styleClass="lower-label" fx:id="lblProfessor" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label fx:id="lblDay" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: AliceBlue;" GridPane.rowSpan="3" />
   </children>
</GridPane>
